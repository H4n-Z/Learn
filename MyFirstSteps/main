public static void main(String[] args) {
    int z1 = 5;
    int z2 = 3;
    boolean a1;

    a1 = z1 ++

    system.out.println(a1)

    a1 = z1 + z2

    system.out.println(a1)

    a1 = z1 - z2

    system.out.println(a1)

    a1 = z1 * z2

    system.out.println(a1)

    a1 = z1 / z2

    system.out.println(a1)

    a1 = z1 % z2

    system.out.println(a1)
}